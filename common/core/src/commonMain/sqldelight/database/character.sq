CREATE TABLE Character(
    name TEXT,
    image TEXT
);

insertCharacter:
INSERT INTO Character(name, image)
VALUES(?,?);

selectAllCharacters:
SELECT Character.*
FROM Character;

selectFirstCharacter:
SELECT Character.*
FROM Character
LIMIT 1;

selectCharacters:
SELECT Character.*
FROM Character
LIMIT :limit
OFFSET :offset;

selectCount:
SELECT COUNT(*)
FROM Character;

removeAllCharacters:
DELETE FROM Character;